    <script>
    mainBlockMinHeight = 0;
    $(document).ready(function () {
        $(window).resize(function () {
            RiskProf.AuthClient.Instance.Resize();
            var navPanel = $('#nav_panel').height();
            var navInfo = $('#nav_info').height();
            var sidebar = $('#sidebar').width();
            $("#sidebar").css({ 'height': 'calc(100vh - 16px - 5px - ' + navPanel + 'px - ' + navInfo + 'px)' });
            $("#head_content").css({ 'width': 'calc(100% - ' + sidebar + 'px)' });
            $("#head_content").css({ 'margin-left': sidebar + 'px' });


            let header = $('#header').outerHeight(true);
            let footer = $('.footer').outerHeight(true);
            let mainBlockHeader = $('.main-block__header').outerHeight(true);
            $(".main-block").css({ 'min-height': `calc(100vh - ${header}px - ${footer}px)` });
            let mainBlock = $('.main-block').outerHeight(true),
                levelJumbotron = +$('.level-jumbotron').outerHeight(true);
            let result = mainBlock - mainBlockHeader - levelJumbotron
            mainBlockMinHeight = result;
            $(".main-block__content").css({ 'height': `${result}px` });
        });
        RiskProf.AuthClient.Instance.InitUserToken(function () {
            RiskProf.AuthClient.Instance.GetHeader();
            RiskProf.AuthClient.Instance.GetNavMenu();
            setTimeout(function () { $(window).resize(); }, 2000);
        });
        RiskProf.Bitrix24.Instance.Init();
        
        $(window).resize();
    });
    //$(document).on('click', '#b24-widget', function () {
    //    RiskProf.Bitrix24.Instance.LiveSupportChat();
    //});

    $(document).on('click', '#js-nav-menu > li > a', function () {
        if (!$("#sidebar").hasClass("active")) {
            $(this).parent().toggleClass('active');
            document.cookie = $(this).parent().attr("id") + '=' + $(this).parent().attr("class") + '; path=/';
        }
    });

    $(document).on('click', 'a[href="#"]', function (e) {
        e.preventDefault();
    });

    $(document).tooltip({
        selector: '[data-toggle="tooltip"]'
    });

    $(document).on('click', '.manualButton', function () {
        var url = 'https://edu.riskprof.ru/learn/showcourse?courseId=5760&isPartial=true';
        ModalDialogV3_showIframe(url, '100%', 'calc(100% - 2rem)');
    });

        $(document).on('click', '.btn-profile-click', function () {
            SpinnerFadeIn();
            var url = 'https://id.riskprof.ru//user/getprofileview';
            var data = {
                'userId': '2984'
            }
            this.UserToken = Cookies.get('RiskProf_UserToken');
            $.ajax({
                "url": url,
                headers: { 'RiskProf_UserToken': this.UserToken },
                "data": data,
                "type": "Post",
                "success": function (data) {
                    ModalDialogV3_show(data);
                },
                "error": function (data) {
                    ModalDialogV3_show(data.responseText);
                }
            });
            SpinnerFadeOut();
            return false;

    });

    $(document).find('.value_global_search').on('keyup', function (e) {
        //Игнорируем клавиши, используемые для смены раскладки клавиатуры (Shift, Alt, Ctrl), и клавиши-стрелки
        if ((e.keyCode >= 16 && e.keyCode <= 18) || (e.keyCode >= 37 && e.keyCode <= 40))
            return;

        var searchList = $('.list_global_search');
        var searchString = $(this).val();

        searchList.css('display', 'none');

        if (searchString.length >= 1)
            RiskProf.Search.Instance.Find(RiskProf.DependencyResolvedManager.Resolved('currentSearchProviders'), searchString, searchList, e.keyCode == 13);
        else
            clearTimeout(RiskProf.Search.Instance._timerId);
    });

    $(document).find('.header-search__form').on('submit', function (e) {
        e.preventDefault();
    });

    $(document).on('click', '#userAdmin', function () {
        let url = 'https://admin.riskprof.ru/setAuthCookie?userToken=132a2182065240b0aaeb10314639c941&amp;returnUrl=https://admin.riskprof.ru/user/GetList';
        ModalDialogV3_showIframe(url, '100%', '60%', false);
    });

    
    </script>
</body>
